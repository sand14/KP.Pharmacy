<Window x:Class="KP.WPF.App.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="700" Width="1000" WindowStartupLocation="CenterScreen" >
    
    <DockPanel LastChildFill="True">
        <ContentControl prism:RegionManager.RegionName="HomeRegion"  />
        <ContentControl prism:RegionManager.RegionName="ContentRegion" Visibility="{Binding LoginVisibilty,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  />
        
    </DockPanel>
</Window>
